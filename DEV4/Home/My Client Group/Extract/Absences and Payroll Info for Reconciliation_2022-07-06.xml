<Extract_Definition><DefinitionName>Absences and Payroll Info for Reconciliation</DefinitionName><XmlTagName>Absences_and_Payroll_Info_for_Reconciliation</XmlTagName><ExtTypeCode>FULLPRF</ExtTypeCode><DefinitionDescription>Extract key absence repository and payroll information to reconcile approved absences with paid absences.</DefinitionDescription><Status>A</Status><BaseDefinitionName>ORA_HWM_ABSENCE_PAYROLL_RECON_INFO</BaseDefinitionName><DateRange>90</DateRange><HistoryChanges>2</HistoryChanges><OutputDateRange>3650</OutputDateRange><IncludeChangesFromLastSuccessfulRun>N</IncludeChangesFromLastSuccessfulRun><Extract_Block><BaseUserEntityName>ORA_HWM_PERSON_ASSIGNMENT_ABSENCE_UE</BaseUserEntityName><BlockName>Person and Assignment Details for Reconciliation Data Group</BlockName><XmlTagName>Person_and_Assignment_Details_for_Reconciliation_Data_Group</XmlTagName><MainBlockFlag>Y</MainBlockFlag><MultiThreadActType>T</MultiThreadActType><UserName>ORA_HWM_PERSON_ASSIGNMENT_ABS_PERSON_ID</UserName><BaseBlockName>ORA_HWM_PERSON_ASSIGNMENT_ABSENCE</BaseBlockName><Block_Criteria><EffectiveStartDate>0001-01-01</EffectiveStartDate><Criteria>EXISTS  (
                SELECT 1
                FROM    anc_per_abs_entries      apae,
                        anc_per_abs_plan_entries apape
                WHERE   apae.approval_status_cd = 'APPROVED'
                 AND    apae.per_absence_entry_id = apape.per_absence_entry_id
                 AND    apae.person_id = target.person_id
                 AND    apape.assignment_id = target.assignment_id
                 AND    target.end_date >= apae.start_date
                 AND    (
                         target.start_date &lt;= apae.end_date
                         OR     (pay_report_utils.get_parameter_value('ORA_HWM_INCLUDE_RETRO') = 'Y'
                                 AND apae.payroll_chg_flag = 'C'
                                 AND TRUNC(apae.last_update_date) BETWEEN target.start_date AND trunc(SYSDATE)
                                )
                        )
                )</Criteria><CriteriaFlag>C</CriteriaFlag><DispCriteriaExp>{EDITADV}</DispCriteriaExp></Block_Criteria><Extract_Record><RecordName>Person and Assignment Details</RecordName><XmlTagName>Person_and_Assignment_Details</XmlTagName><TypeCode>D</TypeCode><ProcessCode>FF</ProcessCode><FlexStructureCode>Person_and_Assignment_Details</FlexStructureCode><Sequence>10</Sequence><LastRunExecutedMode>N</LastRunExecutedMode><OverflowMode>N</OverflowMode><NextBlockId>Absence and Calculation Card Details for Reconciliation</NextBlockId><BaseRecordName>ORA_HWM_PERSON_ASSIGNMENT_ABSENCE</BaseRecordName><EffectiveStartDate>0001-01-01</EffectiveStartDate><Extract_DataElement><StartDate>0001-01-01</StartDate><DataElementName>Person ID</DataElementName><BaseDataElementName>Person ID</BaseDataElementName><ShortCode>Person_ID</ShortCode><TypeCode>F</TypeCode><DataType>N</DataType><XmlTagName>Person_ID</XmlTagName><InformationColumn>1</InformationColumn><InformationLabel>Person_ID</InformationLabel><GroupName>Person and Assignment Details - Person ID</GroupName><BaseGroupName>ORA_HWM_PERSON_ASSIGNMENT_ABS_PERSON_ID</BaseGroupName></Extract_DataElement><Extract_DataElement><StartDate>0001-01-01</StartDate><DataElementName>Legal Employer Name</DataElementName><BaseDataElementName>Legal Employer Name</BaseDataElementName><ShortCode>Legal_Employer_Name</ShortCode><TypeCode>F</TypeCode><DataType>T</DataType><XmlTagName>Legal_Employer_Name</XmlTagName><InformationColumn>3</InformationColumn><InformationLabel>Legal_Employer_Name</InformationLabel><GroupName>Assignment Legal Employer Name</GroupName><BaseGroupName>Assignment Legal Employer Name</BaseGroupName></Extract_DataElement><Extract_DataElement><StartDate>0001-01-01</StartDate><DataElementName>Business Unit Name</DataElementName><BaseDataElementName>Business Unit Name</BaseDataElementName><ShortCode>Business_Unit_Name</ShortCode><TypeCode>F</TypeCode><DataType>T</DataType><XmlTagName>Business_Unit_Name</XmlTagName><Information