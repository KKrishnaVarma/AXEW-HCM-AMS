	AND PAAF.legislation_code NOT IN ('US','CA')
	AND PAAF.PRIMARY_FLAG = 'Y'
--AND PAPF.PERSON_NUMBER='300546'
	AND EXISTS (
		SELECT 1
		FROM PER_EMAIL_ADDRESSES
		WHERE PERSON_ID = PAPF.PERSON_ID
			AND EMAIL_TYPE = 'W1'
			
			AND pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE') BETWEEN DATE_FROM
				AND NVL(DATE_TO,TO_DATE ('31-12-4712','DD-MM-RRRR'))
		)

/*to restrict the WESCO LE employees*/
and not exists(
select 1 from HR_ORGANIZATION_UNITS_F_TL hauft 
where hauft.organization_id = paaf.legal_entity_id 
AND     SYSDATE BETWEEN hauft.effective_start_date AND hauft.effective_end_date
AND hauft.LANGUAGE = 'US' 
