	AND ppos.DATE_START = (
		SELECT MAX(DATE_START)
		FROM PER_PERIODS_OF_SERVICE
		WHERE PERSON_ID = ppos.PERSON_ID
			AND PERIOD_TYPE = 'E'
			AND DATE_START &lt;= pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')
		)
	AND (
		ppos.ACTUAL_TERMINATION_DATE IS NULL
		OR (
			TRUNC(ppos.ACTUAL_TERMINATION_DATE) BETWEEN (pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE') - pay_report_utils.get_parameter_value('INCLUDE_TERM_DAYS'))
				AND (pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE'))
			)
		)
	AND paaf.legislation_code IN (
		SELECT lookup_code
		FROM FND_COMMON_LOOKUPS
		WHERE lookup_type = 'AXE_CONCUR_INT_COUNTRIES'
			AND ENABLED_FLAG = 'Y'
			AND SYSDATE BETWEEN START_DATE_ACTIVE
				AND NVL(END_DATE_ACTIVE, SYSDATE - 1)
		)
	AND paaf.assignment_sequence = (
		SELECT max(paam2.assignment_sequence)
		FROM per_all_assignments_m paam2
		WHERE paam2.assignment_type = 'E'
			AND paaf.person_id = paam2.person_id
		)
