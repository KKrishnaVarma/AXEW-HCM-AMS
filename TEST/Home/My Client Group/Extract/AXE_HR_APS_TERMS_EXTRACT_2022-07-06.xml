OR
paaf.ASSIGNMENT_TYPE = 'P'
/*  AND NOT EXISTS (SELECT
'X' from PER_PERIODS_OF_SERVICE ppos1
 where ppos1.person_id  =ppos.person_id and ppos1.period_type='E' )
               commented wrt INC0362074 */
/*Addded wrt INC0362074 start*/
/* AND
EXISTS
(SELECT 'X'
 FROM HRC_TXN_DATA TXND , HRC_TXN_HEADER TXNH , HRC_TXN_CONSOLE_ENTRY TXNC
 WHERE
 TXNH.TRANSACTION_ID                                                                   = TXND.TRANSACTION_ID
 AND TXNH.TRANSACTION_ID                                                               = TXNC.TRANSACTION_ID
 AND REPLACE(TXNC.PROCESS_CATEGORY,'Agregar trabajador pendiente','Add Pending Worker')='Add Pending Worker'
 AND TXNH.SUBJECT                                                                      = 'PER_PERSONS'
 AND (TXNH.SUBJECT_ID = papf.person_id
 OR(TXNH.SUBJECT_ID = -1
 AND UPPER(TXNC.OBJECT_NAME) IN
		(
		  SELECT  UPPER(PPNF.DISPLAY_NAME)
		  FROM per_person_names_f ppnf
		  WHERE   ppnf.person_id     = papf.person_id
		  AND PPNF.NAME_TYPE = 'GLOBAL'
		  AND TRUNC(pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')) BETWEEN PPNF.EFFECTIVE_START_DATE AND PPNF.EFFECTIVE_END_DATE
		)))
  AND TXND.STATUS='APPROVED'
) */
)
AND ppos.date_start =
(
   SELECT
		  MAX(ppos1.date_start)
   FROM
		  per_periods_of_service ppos1
   WHERE
		  ppos1.person_id       = papf.person_id
		  AND ppos1.date_start &lt;= TRUNC(pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE'))
)
AND ppos.creation_date =
(
       SELECT
              MAX(ppos1.creation_date)
       FROM
              per_periods_of_service ppos1
       WHERE
              ppos1.person_id      = papf.person_id
              AND ppos1.date_start = ppos.date_start
) 
and ppos.action_occurrence_id IN
         (
                select
                       pao.action_occurrence_id
                from
                       PER_ACTION_OCCURRENCES pao
                     , per_actions_vl         pav
                where
                       pav.action_id = pao.action_id
                and UPPER(pav.action_code) IN ( 'INVOLUNTARY_TERMINATION' ,'REDUCTION_FORCE' ,'RETIREMENT' ,'TERMINATION' ,'DEATH')
				AND TRUNC(pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')) BETWEEN PAV.START_DATE AND PAV.END_DATE 
         )
/*Addded wrt INC0362074 end*/
AND paaf.EFFECTIVE_LATEST_CHANGE = 'Y'
AND ppos.ACTUAL_TERMINATION_DATE &lt; pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')
AND paaf.action_code &lt;> 'GLB_TRANSFER'
