AND paaf.ASSIGNMENT_STATUS_TYPE='ACTIVE'
	AND ppos.DATE_START = (
		SELECT MAX(DATE_START)
		FROM PER_PERIODS_OF_SERVICE
		WHERE PERSON_ID = ppos.PERSON_ID
			AND PERIOD_TYPE = 'E'
			AND DATE_START &lt;= pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')
		)
	AND ppos.ACTUAL_TERMINATION_DATE IS NULL		
	AND PAAF.ASSIGNMENT_SEQUENCE = (
		SELECT MAX(PAAM2.ASSIGNMENT_SEQUENCE)
		FROM PER_ALL_ASSIGNMENTS_M PAAM2
		WHERE PAAM2.ASSIGNMENT_TYPE = 'E'
			AND PAAF.PERSON_ID = PAAM2.PERSON_ID
			AND PAAF.effective_start_date = PAAM2.Effective_start_Date
		)
AND NOT EXISTS (
Select 1 from per_all_assignments_m PAAM3
where PAAM3.person_id=PAAF.PERSON_ID
AND PAAM3.ASSIGNMENT_TYPE = 'E'
AND (PAAM3.Union_id IS NOT NULL AND PAAM3.Legislation_Code in ('US','CA'))
and trunc(sysdate) between PAAM3.effective_start_date and PAAM3.effective_end_date
and PAAM3.ASSIGNMENT_STATUS_TYPE ='ACTIVE'
