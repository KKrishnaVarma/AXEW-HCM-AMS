<Extract_Definition><DefinitionName>AXE_HR_ALI_COST_CENTER_EXTRACT</DefinitionName><XmlTagName>AXE_HR_ALI_COST_CENTER_EXTRACT</XmlTagName><ExtTypeCode>HRAR</ExtTypeCode><DateStart>2000-01-01</DateStart><DefinitionDescription>HCM_I043 - HR to AL- I Cornerstone (Cost Center) - Outbound Interface</DefinitionDescription><Status>A</Status><BaseDefinitionName>AXE_HR_ALI_COST_CENTER_EXTRACT</BaseDefinitionName><IncludeChangesFromLastSuccessfulRun>Y</IncludeChangesFromLastSuccessfulRun><Extract_Block><BaseUserEntityName>PER_EXT_SEC_ORGANIZATION_UE</BaseUserEntityName><BlockName>Cost_Center_DG</BlockName><XmlTagName>Cost_Center_DG</XmlTagName><MainBlockFlag>Y</MainBlockFlag><MultiThreadActType>O</MultiThreadActType><UserName>PER_EXT_ORG_ORGANIZATION_ID</UserName><BaseBlockName>Cost_Center_DG300000036916584</BaseBlockName><Block_Criteria><EffectiveStartDate>2000-01-01</EffectiveStartDate><Criteria>hac.classification_code = 'DEPARTMENT'
and hac.STATUS='A'

/*Include AXE Only Employees*/
and org.organization_id in 
(
SELECT paam.organization_id
/*paam.assignment_id ,paam.assignment_number,,paam.legal_entity_id*/
FROM per_all_assignments_m paam
WHERE paam.primary_assignment_flag = 'Y'
and paam.assignment_type         = 'E'
and paam.effective_latest_change = 'Y'
and TRUNC(SYSDATE) between paam.effective_start_date and paam.effective_end_date  
AND EXISTS
(
   SELECT *
   FROM   hr_legal_entities hle
   WHERE  hle.organization_id =paam.legal_entity_id
   AND hle.status = 'A'
   AND hle.classification_code = 'HCM_LEMP'
   AND TRUNC(SYSDATE) BETWEEN hle.effective_start_date AND hle.effective_end_date 
   AND hle.organization_code IN (SELECT hl.lookup_code
								 FROM hcm_lookups hl 
								 WHERE enabled_flag = 'Y' 
								 AND lookup_type = 'AXE_HR_ANIXTER_LEGAL_EMP_LKP' 
								 )                                                              
)
)
