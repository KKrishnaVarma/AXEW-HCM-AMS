<Extract_Definition><DefinitionName>WCC_HR_UBS_STOCK_OPTION_EXTRACT</DefinitionName><XmlTagName>AXEW_UBS_INTERFACE_EXTRACT</XmlTagName><ExtTypeCode>FULLPRF</ExtTypeCode><DateStart>2000-01-01</DateStart><Status>A</Status><BaseDefinitionName>WCC_HR_UBS_STOCK_OPTION_EXTRACT</BaseDefinitionName><IncludeChangesFromLastSuccessfulRun>N</IncludeChangesFromLastSuccessfulRun><Extract_Block><BaseUserEntityName>PER_EXT_PAY_EMPLOYEES_UE</BaseUserEntityName><BlockName>Employee_1</BlockName><XmlTagName>Employee_DG</XmlTagName><MainBlockFlag>Y</MainBlockFlag><MultiThreadActType>O</MultiThreadActType><UserName>PER_EXT_PAY_HR_ASSIGNMENT_ID</UserName><BaseBlockName>Employee_1</BaseBlockName><Block_Criteria><EffectiveStartDate>2000-01-01</EffectiveStartDate><Criteria>paaf.assignment_type = 'E'
AND paaf.primary_work_relation_flag = 'Y'
AND paaf.primary_assignment_flag = 'Y'
AND paaf.effective_latest_change = 'Y'
AND papf.person_number not like '%TEMP%'
AND ppos.date_start = (
						SELECT
							MAX(ppos1.date_start)
						FROM
							per_periods_of_service   ppos1,
							per_all_assignments_m    paam1
						WHERE
							paam1.person_id 						= paaf.person_id          
							AND paam1.period_of_service_id			= ppos1.period_of_service_id
							--AND paam1.business_unit_id			= paaf.business_unit_id
							AND paam1.assignment_type			= 'E'
							AND paam1.primary_assignment_flag		= 'Y'
							AND paam1.primary_work_relation_flag	= 'Y'
							AND trunc(ppos1.date_start)				&lt;= trunc(pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE'))
							AND trunc(pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')) BETWEEN paam1.EFFECTIVE_START_DATE AND paam1.EFFECTIVE_END_DATE
					  )
AND (
		(ppos.actual_termination_date IS NULL)
        OR (trunc(ppos.actual_termination_date) >= (pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE') - pay_report_utils.get_parameter_value('INCLUDE_TERM_DAYS')))
        OR (ppos.actual_termination_date IS NOT NULL
			AND trunc(ppos.last_update_date)	>= (pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE') - pay_report_utils.get_parameter_value('INCLUDE_TERM_DAYS'))
			AND ppos.last_updated_by 			&lt;> 'FUSION_APPS_HCM_ESS_LOADER_APPID')
    )
AND EXISTS (
	SELECT 1
	FROM PER_PEOPLE_EXTRA_INFO_F PPEI
	WHERE 1 = 1
	AND PPEI.PERSON_ID = paaf.person_id
	AND PPEI.INFORMATION_TYPE IN ('AXE_STOCK_LTI')
	AND PPEI.PEI_INFORMATION1 IN ('RSU Only')
	AND pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE') BETWEEN PPEI.EFFECTIVE_START_DATE AND PPEI.EFFECTIVE_END_DATE
)</Criteria><CriteriaFlag>C</CriteriaFlag><DispCriteriaExp>{EDITADV}</DispCriteriaExp></Block_Criteria><Extract_Record><RecordName>EMPLOYEE_DETAIL</RecordName><XmlTagName>EMPLOYEE_DETAIL</XmlTagName><TypeCode>D</TypeCode><ProcessCode>FF</ProcessCode><FlexStructureCode>EMPLOYEE_DETAIL</FlexStructureCode><Sequence>1</Sequence><LastRunExecutedMode>N</LastRunExecutedMode><OverflowMode>N</OverflowMode><NextBlockId>Seniority_Dates</NextBlockId><BaseRecordName>EMPLOYEE_DETAIL300000077278294</BaseRecordName><EffectiveStartDate>2000-01-01</EffectiveStartDate><Extract_DataElement><StartDate>2000-01-01</StartDate><EndDate>4712-12-31</EndDate><DataElementName>PARTICIPANT_ID</DataElementName><BaseDataElementName>PARTICIPANT_ID</BaseDataElementName><ShortCode>PARTICIPANT_ID</ShortCode><TypeCode>F</TypeCode><DataType>N</DataType><XmlTagName>PARTICIPANT_ID</XmlTagName><InformationColumn>1</InformationColumn><InformationLabel>PARTICIPANT_ID</InformationLabel><GroupName>Extract Employee Person Number</GroupName><BaseGroupName>PER_EXT_PAY_PERSON_NUMBER</BaseGroupName></Extract_DataElement><Extract_DataElement><StartDate>2000-01-01</StartDate><EndDate>4712-12-31</EndDate><DataElementName>TERMINATION_DATE</DataElementName><BaseDataElementName>TERMINATION_DATE</BaseDataElementName><ShortCode>TERMINATION_DATE</ShortCode><TypeCode>F</TypeCode><DataType>D</DataType><XmlTagName>TERMINATION_DATE</XmlTagName><InformationColumn>2</InformationColumn><InformationLabel>TERMINATION_DATE</InformationLabel><GroupName>Employee Termination Date</GroupName><BaseGroupName>Employee Termination Date</BaseGroupName></Extract_DataElement><Extract_DataElement><StartDate>2000-01-01</StartDate><EndDate>4712-12-31</EndDate><DataElementName>SOC_SEC_NUMBER</DataElementName><BaseDataElementName>SOC_SEC_NUMBER</BaseDataElementName><ShortCode>SOC_SEC_NUMBER</ShortCode><TypeCode>F</TypeCode><DataType>T</DataType><XmlTagName>SOC_SEC_NUMBER</XmlTagName><InformationColumn>3</InformationColumn><InformationLabel>SOC_SEC_NU