<Extract_Definition><DefinitionName>AXE_HR_RC_MRD_PURGE_EMP_EXTRACT</DefinitionName><XmlTagName>AXE_HR_RC_MRD_PURGE_EMP_EXTRACT</XmlTagName><ExtTypeCode>HRAR</ExtTypeCode><DateStart>2000-01-01</DateStart><DefinitionDescription>HCM_I023 - HR to RedCarpet Maintain Referential Data (Purge User) Outbound Interface</DefinitionDescription><Status>A</Status><BaseDefinitionName>AXE_HR_RC_MRD_PURGE_EMP_EXTRACT</BaseDefinitionName><IncludeChangesFromLastSuccessfulRun>Y</IncludeChangesFromLastSuccessfulRun><Extract_Block><BaseUserEntityName>PER_EXT_PAY_EMPLOYEES_UE</BaseUserEntityName><BlockName>Employee</BlockName><XmlTagName>Employee</XmlTagName><MainBlockFlag>Y</MainBlockFlag><MultiThreadActType>O</MultiThreadActType><UserName>PER_EXT_PAY_HR_ASSIGNMENT_ID</UserName><BaseBlockName>Employee300000036927685</BaseBlockName><Block_Criteria><EffectiveStartDate>2000-01-01</EffectiveStartDate><Criteria>(paaf.ASSIGNMENT_STATUS_TYPE = 'INACTIVE'
and ppos.person_id=paaf.person_id
   
   AND  ((ppos.ACTUAL_TERMINATION_DATE BETWEEN (pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE') - pay_report_utils.get_parameter_value('TERMINATION_LOOK_BACK_DAYS')) AND pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE'))
	 OR 
	 ((TRUNC(ppos.LAST_UPDATE_DATE) BETWEEN (pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE') - pay_report_utils.get_parameter_value('TERMINATION_LOOK_BACK_DAYS')) AND pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE'))
	     AND ppos.ACTUAL_TERMINATION_DATE &lt; pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')))
 
 
and pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE') between paaf.effective_start_date and paaf.effective_end_date 
 and
 ppos.date_start =
 (
        select
               max(ppos1.date_start)
        from
               PER_PERIODS_OF_SERVICE ppos1  
        where
               ppos1.PERSON_ID       = ppos.PERSON_ID
               AND ppos1.date_start &lt; pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')
 )  
 AND NOT EXISTS
 (SELECT ppos2.person_id from per_periods_of_service ppos2 where
 ((ppos2.ACTUAL_TERMINATION_DATE is NULL )
 or
 (ppos2.ACTUAL_TERMINATION_DATE >pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')))
 and ppos2.person_id=ppos.person_id))


/*to restrict the WESCO LE employees*/
and not exists(
select 1 from HR_ORGANIZATION_UNITS_F_TL hauft 
where hauft.organization_id = paaf.legal_entity_id 
AND     SYSDATE BETWEEN hauft.effective_start_date AND hauft.effective_end_date
AND hauft.LANGUAGE = 'US' 
and hauft.name = 'WESCO Blended Leader Holding Entity'
)</Criteria><CriteriaFlag>C</CriteriaFlag><DispCriteriaExp>{EDITADV}{TERMINATION_LOOK_BACK_DAYS}</DispCriteriaExp></Block_Criteria><Extract_Record><RecordName>Employee Record</RecordName><XmlTagName>Employee_Record</XmlTagName><TypeCode>D</TypeCode><ProcessCode>FF</ProcessCode><FlexStructureCode>Employee_Record</FlexStructureCode><Sequence>10</Sequence><LastRunExecutedMode>N</LastRunExecutedMode><OverflowMode>N</OverflowMode><BaseRecordName>Employee_Record300000036927775</BaseRecordName><EffectiveStartDate>2000-01-01</EffectiveStartDate><Extract_DataElement><StartDate>2000-01-01</StartDate><DataElementName>UserToEdit_LoginID</DataElementName><BaseDataElementName>UserToEdit_LoginID</BaseDataElementName><ShortCode>UserToEdit_LoginID</ShortCode><TypeCode>F</TypeCode><DataType>T</DataType><XmlTagName>UserToEdit_LoginID</XmlTagName><InformationColumn>1</InformationColumn><InformationLabel>UserToEdit_LoginID</InformationLabel><GroupName>Extract Employee Person Number</GroupName><BaseGroupName>PER_EXT_PAY_PERSON_NUMBER</BaseGroupName></Extract_DataElement><Extract_DataElement><StartDate>2000-01-01</StartDate><DataElementName>Person ID</DataElementName><BaseDataElementName>Person ID</BaseDataElementName><ShortCode>Person_ID</ShortCode><TypeCode>F</TypeCode><DataType>N</DataType><XmlTagName>Person_ID</XmlTagName><InformationColumn>10</InformationColumn><InformationLabel>Person_ID</InformationLabel><GroupName>Assignment Person ID</GroupName><BaseGroupName>Assignment Person ID</BaseGroupName><KeyFlag>Y</KeyFlag></Extract_DataElement><Extract_DataElement><StartDate>2000-01-01</StartDate><EndDate>4712-12-31</EndDate><DataElementName>Purge Start Date</DataElementName><BaseDataElementName>Purge Start Date</BaseDataElementName><ShortCode>Purge_Start_Date</ShortCode><TypeCode>R</TypeCode><DataType>T</DataType><XmlTagName>Purge_Start_Date</XmlTagName><InformationColumn>15</InformationColumn><InformationLabel>Purge_Start_Date</InformationLabel><FormulaName>AXE_RC_PURGE_USER_DATE_FF</F