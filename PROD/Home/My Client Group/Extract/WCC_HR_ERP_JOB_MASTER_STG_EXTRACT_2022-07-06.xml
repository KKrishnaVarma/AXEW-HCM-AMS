<Extract_Definition><DefinitionName>WCC_HR_ERP_JOB_MASTER_STG_EXTRACT</DefinitionName><XmlTagName>WCC_HR_ERP_JOB_MASTER_STG_EXTRACT</XmlTagName><ExtTypeCode>HRAR</ExtTypeCode><DateStart>2000-01-01</DateStart><Status>A</Status><BaseDefinitionName>WCC_HR_ERP_JOB_MASTER_STG_EXTRACT</BaseDefinitionName><IncludeChangesFromLastSuccessfulRun>Y</IncludeChangesFromLastSuccessfulRun><Extract_Block><BaseUserEntityName>PER_EXT_SEC_JOB_UE</BaseUserEntityName><BlockName>JOB_DG</BlockName><XmlTagName>JOB_DG</XmlTagName><MainBlockFlag>Y</MainBlockFlag><MultiThreadActType>O</MultiThreadActType><UserName>PER_EXT_JOB_JOB_ID</UserName><BaseBlockName>JOB_DG</BaseBlockName><Block_Criteria><EffectiveStartDate>2000-01-01</EffectiveStartDate><Criteria>job.active_status = 'A'
AND job.job_id IN
(
SELECT paam1.JOB_ID
FROM per_all_assignments_m    paam1
WHERE 1 = 1
AND paam1.assignment_type		   = 'E'
AND paam1.primary_assignment_flag  = 'Y'
AND paam1.primary_work_relation_flag  = 'Y'
AND paam1.legislation_code            = 'US'
AND trunc(pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')) BETWEEN paam1.EFFECTIVE_START_DATE AND paam1.EFFECTIVE_END_DATE
AND paam1.legal_entity_id IN (
								 SELECT ORGANIZATION_ID
								 FROM HR_LEGAL_ENTITIES
								 WHERE 1 = 1
								 AND CLASSIFICATION_CODE = 'HCM_LEMP'
								 AND trunc(pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')) BETWEEN EFFECTIVE_START_DATE AND EFFECTIVE_END_DATE
								 AND NAME IN (SELECT MEANING FROM HCM_LOOKUPS WHERE ENABLED_FLAG = 'Y' AND LOOKUP_TYPE = 'WCC_HR_WESCO_LEGAL_EMP_LKP')
							)
