<Extract_Definition><DefinitionName>AXE_HR_I9_FORMS_EXTRACT</DefinitionName><XmlTagName>AXE_HR_I9-FORMS_EXTRACT</XmlTagName><ExtTypeCode>HRAR</ExtTypeCode><DateStart>1951-01-01</DateStart><DefinitionDescription>HR to I9 Forms Quarterly Terminations (for eVerify) - Outbound Interface</DefinitionDescription><Status>A</Status><BaseDefinitionName>AXE_HR_I9_FORMS_EXTRACT</BaseDefinitionName><IncludeChangesFromLastSuccessfulRun>N</IncludeChangesFromLastSuccessfulRun><Extract_Block><BaseUserEntityName>PER_EXT_PAY_EMPLOYEES_UE</BaseUserEntityName><BlockName>Employee</BlockName><XmlTagName>Employee</XmlTagName><MainBlockFlag>Y</MainBlockFlag><BaseBlockName>Employee300000036935148</BaseBlockName><Block_Criteria><EffectiveStartDate>1951-01-01</EffectiveStartDate><Criteria>paaf.LEGISLATION_CODE IN ('US','PR')
and paaf.assignment_type='E'
and paaf.action_code&lt;>'GLB_TRANSFER'
AND ppos.ACTUAL_TERMINATION_DATE IS NOT NULL
AND(
	(ppos.LAST_UPDATE_DATE  >=TRUNC(pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE'), 'Q' ) 
	 AND ppos.LAST_UPDATE_DATE &lt;=pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')
	) OR 
       (ppos.ACTUAL_TERMINATION_DATE  >=TRUNC(pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE'), 'Q' ) 
        AND ppos.ACTUAL_TERMINATION_DATE &lt;=pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')
	)
)
AND PPOS.DATE_START=(SELECT MAX(PPOS1.DATE_START) 
					 FROM PER_PERIODS_OF_SERVICE PPOS1 
					 WHERE PPOS1.PERSON_ID = PPOS.PERSON_ID 
					 AND PPOS1.DATE_START &lt;=pay_report_utils.get_parameter_value_date('EFFECTIVE_DATE')
					 )
/*Include AXE Only Employees*/
AND EXISTS
(
   SELECT 'x'
   FROM   hr_legal_entities hle
   WHERE  hle.organization_id = paaf.legal_entity_id
   AND status = 'A'
   AND classification_code = 'HCM_LEMP'
   AND TRUNC(SYSDATE) BETWEEN hle.effective_start_date AND hle.effective_end_date 
   AND hle.organization_code IN (SELECT hl.lookup_code
							    FROM hcm_lookups hl 
							    WHERE enabled_flag = 'Y' 
							     AND lookup_type = 'AXE_HR_ANIXTER_LEGAL_EMP_LKP' 
							    )                                                                
)
